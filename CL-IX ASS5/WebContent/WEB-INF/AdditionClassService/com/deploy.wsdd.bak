<!-- Use this file to deploy some handlers/chains and services      -->
<!-- Two ways to do this:                                           -->
<!--   java org.apache.axis.client.AdminClient deploy.wsdd          -->
<!--      after the axis server is running                          -->
<!-- or                                                             -->
<!--   java org.apache.axis.utils.Admin client|server deploy.wsdd   -->
<!--      from the same directory that the Axis engine runs         -->

<deployment
    xmlns="http://xml.apache.org/axis/wsdd/"
    xmlns:java="http://xml.apache.org/axis/wsdd/providers/java">

  <!-- Services from AdditionClassService WSDL service -->

  <service name="AdditionClass" provider="java:RPC" style="wrapped" use="literal">
      <parameter name="wsdlTargetNamespace" value="http://com"/>
      <parameter name="wsdlServiceElement" value="AdditionClassService"/>
      <parameter name="schemaQualified" value="http://com"/>
      <parameter name="wsdlServicePort" value="AdditionClass"/>
      <parameter name="className" value="com.AdditionClassSoapBindingImpl"/>
      <parameter name="wsdlPortType" value="AdditionClass"/>
      <parameter name="typeMappingVersion" value="1.2"/>
      <operation name="addFunc" qname="operNS:addFunc" xmlns:operNS="http://com" returnQName="retNS:addFuncReturn" xmlns:retNS="http://com" returnType="rtns:string" xmlns:rtns="http://www.w3.org/2001/XMLSchema" soapAction="" >
        <parameter qname="pns:a" xmlns:pns="http://com" type="tns:int" xmlns:tns="http://www.w3.org/2001/XMLSchema"/>
        <parameter qname="pns:b" xmlns:pns="http://com" type="tns:int" xmlns:tns="http://www.w3.org/2001/XMLSchema"/>
      </operation>
      <parameter name="allowedMethods" value="addFunc"/>

  </service>
</deployment>
